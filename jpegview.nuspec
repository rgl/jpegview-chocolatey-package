<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>jpegview</id>
    <version>1.0.37</version>
    <title>JPEGView</title>
    <authors>David Kleiner</authors>
    <owners>Rui Lopes</owners>
    <licenseUrl>https://www.gnu.org/licenses/gpl-2.0.html</licenseUrl>
    <packageSourceUrl>https://github.com/rgl/jpegview-chocolatey-package</packageSourceUrl>
    <projectUrl>https://sourceforge.net/projects/jpegview/</projectUrl>
    <iconUrl>https://c.fsdn.com/allura/p/jpegview/icon</iconUrl>
